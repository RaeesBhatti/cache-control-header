{
  "name": "cache-control-header",
  "version": "0.7.0",
  "description": "Utility function that outputs Cache-Control header string based on given parameters",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc --project tsconfig.mjs.json && npx tsc --project tsconfig.cjs.json && npx tsc --declaration --emitDeclarationOnly"
  },
  "keywords": [
    "cache",
    "cache-control",
    "cache",
    "headers",
    "max-age",
    "maxage",
    "s-maxage",
    "smaxage",
    "swr",
    "stale-while-revalidate",
    "staleWhileRevalidate"
  ],
  "author": "Raees Iqbal",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "devDependencies": {
    "typescript": "^4.9.4"
  },
  "repository": {
    "url": "https://github.com/RaeesBhatti/cache-control-header.git"
  },
  "dependencies": {
    "case-shift": "^2.5.3"
  }
